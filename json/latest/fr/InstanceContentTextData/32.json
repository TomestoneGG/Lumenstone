  },
  {
    "RowId": 41818,
    "Text": "Aaaaaaaah!"
  },
  {
    "RowId": 41819,
    "Text": "Ryne!"
  },
  {
    "RowId": 41820,
    "Text": "Elle tiendra pas longtemps \u00E0 ce rythme!"
  },
  {
    "RowId": 41821,
    "Text": "Raaaah!!"
  },
  {
    "RowId": 41822,
    "Text": "D\u00E9phasage!"
