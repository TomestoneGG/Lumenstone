  },
  {
    "RowId": 41471,
    "Text": "微塵がくれ発動まで\u30002\u3000秒！"
  },
  {
    "RowId": 41472,
    "Text": "微塵がくれ発動まで\u30001\u3000秒！"
  },
  {
    "RowId": 41473,
    "Text": "ぐ～る……"
  },
  {
    "RowId": 41474,
    "Text": "ドォーーーーーン！！！！！"
  },
  {
    "RowId": 41475,
    "Text": "羅刹七星拳ンン！！"
  },
  {
    "RowId": 41476,
    "Text": "微塵がくれ発動まで\u300030\u3000秒！"
  },
