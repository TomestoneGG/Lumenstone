  },
  {
    "RowId": 4803,
    "Text": "BRENNEN, BRENNEN! ALLES BRENNEN!"
  },
  {
    "RowId": 4804,
    "Text": "NEEEIIIIN! MEINE FLAMMEN!!!"
  },
  {
    "RowId": 4805,
    "Text": "Graarrr! Ich Biggy! Groooßer, staaarker Superbiggy!"
  },
  {
    "RowId": 4806,
    "Text": "Muskeln abfallen! Gaaar nicht gut!"
  },
  {
    "RowId": 4807,
    "Text": "Biggy weiterkääämpfen!"
  },
  {
    "RowId": 4808,
    "Text": "Biggy auch ooohne Muskeln stark! Ihr schon seeehen!"
  },
  {
    "RowId": 4809,
    "Text": "Biggy am Endeee! Wie gemeeein!"
  },
