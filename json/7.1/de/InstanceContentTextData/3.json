    "Text": "Ravanas Kampfgeist"
  },
  {
    "RowId": 8401,
    "Text": "Ein Tanz im Takt des singenden Stahls!"
  },
  {
    "RowId": 8402,
    "Text": "Wild wie das Feuer!"
  },
  {
    "RowId": 8403,
    "Text": "Stark wie die Erde!"
  },
  {
    "RowId": 8404,
    "Text": "Erhebe dich, Seele des Kampfes!"
  },
  {
    "RowId": 8405,
    "Text": "Macht euch bereit!"
  },
  {
    "RowId": 8406,
    "Text": "Ruhm der Kolonie! "
  },
  {
    "RowId": 8407,
    "Text": "Ich bin besiegt ..."
  },
  {
    "RowId": 8408,
    "Text": "Ruhm der Kolonie! "
